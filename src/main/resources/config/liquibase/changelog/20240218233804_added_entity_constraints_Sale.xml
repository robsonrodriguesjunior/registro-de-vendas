<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Sale.
    -->
    <changeSet id="20240218233804-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="sale"
                                 constraintName="fk_sale__client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"
                                 />

        <addForeignKeyConstraint baseColumnNames="seller_id"
                                 baseTableName="sale"
                                 constraintName="fk_sale__seller_id"
                                 referencedColumnNames="id"
                                 referencedTableName="collaborator"
                                 />
    </changeSet>
</databaseChangeLog>
